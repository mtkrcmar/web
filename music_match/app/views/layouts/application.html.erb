<!DOCTYPE html>
<html>
<head>
  <title>MusicMatch</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= yield %>


<nav class="navbar navbar-inverse navbar-fixed-top">
  <div class="container">
    <p class="navbar-brand">Application Title</p>
    <%= link_to "Home", "/static_pages/home", :class => "btn btn-primary navbar-btn" %>
    <%= link_to "About", "/static_pages/about", :class => "btn btn-primary navbar-btn" %>
    <%= link_to "Contact", "/static_pages/contact", :class => "btn btn-primary navbar-btn" %>
    <%= link_to "License", "/static_pages/license", :class => "btn btn-primary navbar-btn" %>

    <p class="navbar-text navbar-right">
      <% if user_signed_in? %>
          Logged in as: <%= current_user.email %>
          <br>Not you?
          <%= link_to('Logout', destroy_user_session_path, :method => :delete) %>
      <% else %>
          <%= link_to('Login', new_user_session_path)  %>
      <% end %>
      </div>
</nav>

<br><br><br><br>

</body>
</html>
